{%- liquid 

	assign handles = ""
	
	for manufacturer in linklists['manufacturers'].links
	
		assign handle_with_car_mats_added = handle | append: "-car-mats"
	
		if manufacturer.object.handle == handle_with_car_mats_added
		
			for model in manufacturer.links
			
				assign handles = handles | append: "," | append: model.object.handle
	
			endfor
		
		endif
	
	endfor
	
	assign handles = handles | remove_first: ","
	
	echo handles
-%}