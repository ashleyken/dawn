{% comment %}

Slideshow Section (PRO) 

by ezfycode.com

{% endcomment %}

{% assign dummy_image = 'data:image/svg+xml;base64,' %}
{% assign loader_background_color = section.settings.loader_background_color %}

{% assign fade = section.settings.fade %}
{% assign parallax = section.settings.parallax %}
{% assign text_animation = section.settings.text_animation %}
{% assign zoom_animation = section.settings.zoom_animation %}


{% assign autoplay = section.settings.autoplay %}
{% assign autoplay_speed = section.settings.autoplay_speed %}

{% assign dots_style = section.settings.dots_style %}
{% assign dots_color = section.settings.dots_color %}
{% assign dot_size = section.settings.dot_size %}
{% assign dot_size_selected = section.settings.dot_size_selected %}

{% assign arrows_style = section.settings.arrows_style %}
{% assign arrows_style_mobile = section.settings.arrows_style_mobile %}
{% assign arrow_background_color = section.settings.arrow_background_color %}
{% assign arrow_chevron_color = section.settings.arrow_chevron_color %}
{% assign arrow_size = section.settings.arrow_size %}
{% assign arrow_chevron_size = section.settings.arrow_chevron_size %}
{% assign arrow_size_mobile = section.settings.arrow_size_mobile %}
{% assign arrow_chevron_size_mobile = section.settings.arrow_chevron_size_mobile %}


{% capture has_dots %}
  {% if dots_style == 'bars' or dots_style == 'dots' %}
  true
  {% else %}
  false
  {% endif %}
{% endcapture %}


{% assign section_height = section.settings.section_height %}
{% assign section_height_mobile = section.settings.section_height_mobile %}

{%- assign natural_height = false -%}
{%- assign natural_height_mobile = false -%}

{%- if section_height == 'natural' or section_height_mobile == 'auto' -%}

{% comment %}
    Get first image's aspect ratio
{% endcomment %}

{%- for block in section.blocks limit: 1 -%}
    {%- if block.settings.image == blank -%}

      {% assign section_height = '450px' %}
      {% assign section_height_mobile = '250px' %}

	{% else %}
    {%- if section_height == 'natural' %}
        {%- assign natural_height = true -%}
        {%- capture natural_height_ratio -%}{{ block.settings.image.aspect_ratio }}{% endcapture %}
    {%- endif -%}
    {%- endif -%}

    
    {%- if section_height_mobile == 'auto' -%}
        {%- assign natural_height_mobile = true -%}
        {%- capture natural_height_ratio_mobile -%}
        {% if block.settings.image_mobile != blank %}
        {{ block.settings.image_mobile.aspect_ratio }}
        {% else %}
        {{ block.settings.image.aspect_ratio }}
        {% endif %}
        {% endcapture %}

    {%- endif -%}


{%- endfor -%}

{%- endif -%}

{% if fade == false %}
{% assign parallax = false %}
{% endif %}




<style>

  .EzfySlideshow-container--loading *{
  	opacity: 0;
  }
</style>

<div
     class="EzfySlideshowOptions"
     data-ezfy-slideshow-section-id="{{ section.id }}"
     data-autoplay="{{ autoplay }}"
     data-autoplay-speed="{{ autoplay_speed }}"
	 data-enable-parallax="{{ parallax }}"
     data-dots-style="{{ dots_style }}"
     data-enable-fade="{{ fade }}"
     style="display:none;height:0px;width:0px;"
     ></div>

<div 
     data-ezfy-slideshow-section-id="{{ section.id }}"
     class="EzfySlideshow-container 
            EzfySlideshow-container--loading
            EzfySlideshow-container--dots-style-{{ dots_style }}
            EzfySlideshow-container--{{ section.id }}"
     
     data-arrows-style="{{ arrows_style }}"
     data-arrows-style-mobile="{{ arrows_style_mobile }}"
     data-has-dots="{{ has_dots | strip }}"
     data-dots-style="{{ dots_style }}">

<div data-slideshow-section-id="{{ section.id }}" 
     class="EzfySlideshow--loading 
            EzfySlideshow 
            EzfySlideshow--{{ section.id }}
            {% if natural_height %}EzfySlideshow-cell--natural-desktop{% endif %}
            {% if natural_mobile_height %} EzfySlideshow-cell--natural-mobile{% endif %}">

  
  {% for block in section.blocks %}
  
  {% assign button_text = block.settings.button_text %}
  {% assign button_link = block.settings.button_link %}
  {% assign button_text_2 = block.settings.button_text_2 %}
  {% assign button_link_2 = block.settings.button_link_2 %}
  
  {% capture button_background_color %}
  {% if block.settings.button_background_color == blank %}
  "transparent"
  {% else %}
  {{ block.settings.button_background_color }}
  {% endif %}
  {% endcapture %}

{% assign button_text_color = block.settings.button_text_color %}
{% assign button_border_color = block.settings.button_border_color %}
  
  {% capture button_background_color_2 %}
  {% if block.settings.button_background_color_2 == blank %}
  "transparent"
  {% else %}
  {{ block.settings.button_background_color_2 }}
  {% endif %}
  {% endcapture %}
{% assign button_text_color_2 = block.settings.button_text_color_2 %}
{% assign button_border_color_2 = block.settings.button_border_color_2 %}
  
  {% assign image_link = block.settings.image_link %}
  {% assign image_link_size = image_link | size %}
  
    <div class="EzfySlideshow-cell EzfySlideshow-cell--{{block.id}}">

            
	  {% assign is_empty = true %}
	  {% assign image_desktop = '' %}
      {% assign image_mobile = '' %}
      
      
      {% if block.settings.image != blank %}
      	  {%- assign image_desktop = block.settings.image -%}
 		  {% assign is_empty = false %}
      {% endif %}
      
      {% if block.settings.image_mobile != blank %}
      	{%- assign image_mobile = block.settings.image_mobile -%}
      	{% assign is_empty = false %}
      {% else %}
      	{% assign image_mobile = image_desktop %}
      {% endif %}
      
         	
      {% if is_empty %}

      
            <img class=" EzfySlideshow-image EzfySlideshow-image--dummy"
           loading="lazy"
           src="{{ dummy_image }}"

           data-sizes="auto"
           alt="{{ image_desktop.alt | escape }}"
           style="" />

      {% else %}
      

      {% if parallax %}
    	<div class="EzfySlideshow-parallax EzfySlideshow-parallax--desktop lazyload" 
             data-sizes="auto"
             data-bg-image="{{ image_desktop | img_url: '3000x' }}"
			data-bgset="
            {{ image_desktop | img_url: '3000x' }} [(min-width: 2500px)] |
            {{ image_desktop | img_url: '2500x' }} [(min-width: 2000px)] |
            {{ image_desktop | img_url: '2000x' }} [(min-width: 1500px)] | 
            {{ image_desktop | img_url: '1700x' }} [(min-width: 1200px)] | 
            {{ image_desktop | img_url: '1500x' }} [(min-width: 1000px)] | 
            {{ image_desktop | img_url: '1400x' }} [(min-width: 900px)]  |
            {{ image_desktop | img_url: '1200x' }} [(max-width: 900px)]  |
        "
             style="background-image: url('{{ image_desktop | img_url: '1x1' }}');">
      {% endif %}


   
      
      <img class="lazyload EzfySlideshow-image EzfySlideshow-image--desktop"
      
           src="{{ image_desktop | img_url: '1x1' }}"
           
           data-srcset="
                    {{ image_desktop | img_url: '3000x' }} 3000w,
                    {{ image_desktop | img_url: '2700x' }} 2700w,
                    {{ image_desktop | img_url: '2450x' }} 2450w,
                    {{ image_desktop | img_url: '2048x' }} 2048w,
                    {{ image_desktop | img_url: '1728x' }} 1728w,
                    {{ image_desktop | img_url: '1512x' }} 1512w,
                    {{ image_desktop | img_url: '1296x' }} 1296w,
                    {{ image_desktop | img_url: '1080x' }} 1080w,
                    {{ image_desktop | img_url: '900x' }} 900w
                        "
           data-aspectratio="{{ image_desktop.aspect_ratio }}"
           data-sizes="65vw"
           alt="{{ image_desktop.alt | escape }}"
           style="" />
		
      {% if parallax %}
        </div>
      {% endif %}

      

               
    {% if parallax %}
    	<div class="EzfySlideshow-parallax EzfySlideshow-parallax--mobile lazyload" 
             
                          data-sizes="auto"
			data-bgset="
            {{ image_mobile | img_url: '800x' }}"
             data-bg-image="{{ image_desktop | img_url: '800x' }}"
             style="background-image: url('{{ image_mobile | img_url: '1x1' }}');">
    {% endif %}
      <img class="lazyload EzfySlideshow-image EzfySlideshow-image--mobile"
           loading="lazy"
           src="{{ image_mobile | img_url: '1x1' }}"
           data-src="{{ image_mobile | img_url: '800x' }}"
           data-aspectratio="{{ image_mobile.image.aspect_ratio }}"
           data-sizes="auto"
           alt="{{ image_mobile.image.alt | escape }}"
           style="" />
    {% if parallax %}
        </div>
    {% endif %}
      
      {% endif %}

       {% if image_link_size > 0 %}
      <a class="EzfySlideshow-clickable" href="{{ shop.url }}{{ image_link }}"></a>
      {% endif %}
      
      
      <div class="EzfySlideshow-textbox-container--parent">
        <div class="EzfySlideshow-textbox-container--child">
          <div class="EzfySlideshow-textbox {{ block.settings.text_align }}">
            <div class="EzfySlideshow-textbox-container--alignment">
              <div class="EzfySlideshow-title-container"> 
                 <h2 class="EzfySlideshow-title">{{ block.settings.title | newline_to_br }}</h2>
                </div>
              
              <div class="EzfySlideshow-subtitle-container"> 
              	<p class="EzfySlideshow-subtitle">{{ block.settings.subtitle | newline_to_br }}</p>
              </div>
              
              <div class="EzfySlideshow-buttons"> 
                {% if button_text != "" %} 
        		<a 
                    class="EzfySlideshow-button EzfySlideshow-button--1"
                   href="{%if button_link != blank %}{{ button_link }}{% else %}{{ shop.url }}{% endif %}">{{ button_text }}</a>				
               
{% endif %}
                {% if button_text_2 != "" %}
        		<a 
                    class="EzfySlideshow-button EzfySlideshow-button--2"
                   href="{%if button_link_2 != blank %}{{ button_link_2}}{% else %}{{ shop.url }}{% endif %}">{{ button_text_2 }}</a>
				{% endif %}
              </div>
            </div>
          </div>
        </div>
      </div>
  
      
      
      
      <style>        
        .EzfySlideshow-cell--{{block.id}}:after{
            content:'';
            width: 100%;
            height: 100%;
          	position: absolute;
            top: 0;
            left:0;
            right: 0;
            bottom: 0;
          	pointer-events: none;
          
          	background: {{ block.settings.overlay_color }};
          	opacity: .{{ block.settings.overlay_opacity }};
        }
        
        .EzfySlideshow-cell--{{block.id}} .EzfySlideshow-image{
          
          {% if block.settings.focal_point == 'full_width' %}
			object-fit: contain;
          {% else %}
        	object-position: {{ block.settings.focal_point }};
          {% endif %}
        }
        
        {% if block.settings.focal_point == 'full_width' %}
         .EzfySlideshow-cell--{{block.id}} .EzfySlideshow-parallax--desktop{
        	background-size: contain;
        }
        {% endif %}
        
        .EzfySlideshow-cell--{{block.id}} .EzfySlideshow-parallax--desktop{
        	background-position: {{ block.settings.focal_point }};
        }
        
        .EzfySlideshow-cell--{{ block.id}} .EzfySlideshow-title{
          font-size: {{ block.settings.title_font_size }}px;
        }
        
        .EzfySlideshow-cell--{{ block.id}} .EzfySlideshow-subtitle{
          font-size: {{ block.settings.subtitle_font_size }}px;
        }
        
        .EzfySlideshow-cell--{{ block.id }} .EzfySlideshow-button{
          font-size: {{ block.settings.button_text_size }}px;
        }
        
        @media (max-width: 749px){
          
          .EzfySlideshow-cell--{{block.id}} .EzfySlideshow-image{
          {% if block.settings.focal_point_mobile == 'full_width' %}
			object-fit: contain;
            object-position: center;
          {% else %}
         object-position: {{ block.settings.focal_point_mobile }};
          {% endif %}
            
          }
          
        {% if block.settings.focal_point_mobile == 'full_width' %}
          
          
          
         .EzfySlideshow-cell--{{block.id}} .EzfySlideshow-parallax--mobile{
              
    background-position: initial;
        	background-size: contain;
        }
        {% endif %}
          
           .EzfySlideshow-cell--{{block.id}} .EzfySlideshow-parallax--mobile{
        	background-position: {{ block.settings.focal_point_mobile }};
        	}

          .EzfySlideshow-cell--{{ block.id}} .EzfySlideshow-title{
            font-size: {{ block.settings.title_font_size_mobile }}px;
          }
          
          .EzfySlideshow-cell--{{ block.id}} .EzfySlideshow-subtitle{
          	font-size: {{ block.settings.subtitle_font_size_mobile }}px;
          }
          
          .EzfySlideshow-cell--{{ block.id }} .EzfySlideshow-button{
            font-size: {{ block.settings.button_text_size_mobile }}px;
          }
        }
        
        {% comment %} Text shadow {% endcomment %}

        {% assign text_shadow = block.settings.text_shadow %}

        {% if text_shadow != 0 %}        
        .EzfySlideshow-cell--{{ block.id}} .EzfySlideshow-textbox-container--alignment:after {
            content: "";
            position: absolute;
            top: 0px;
            right: 0px;
            bottom: 0px;
            left: 0px;
            z-index: -1;
          
          	
          	background: radial-gradient({{ block.settings.text_shadow_color | color_to_rgb }} 0%, transparent 60%);
          	opacity: {% if text_shadow == 100 %}1{% else %} .{{ text_shadow }} {% endif %};
            margin: -100px -200px;
        }
        {% endif %}
        
        {% comment %} Button {% endcomment %}
        
        
        .EzfySlideshow-cell--{{ block.id}} .EzfySlideshow-button--1,
        .EzfySlideshow-cell--{{ block.id}} .EzfySlideshow-button--1:active,
        .EzfySlideshow-cell--{{ block.id}} .EzfySlideshow-button--1:hover,
        .EzfySlideshow-cell--{{ block.id}} .EzfySlideshow-button--1:focus{
          background-color: {{ button_background_color }};
          color: {{ button_text_color }};
          border-color: {{ button_border_color }};
        }
        
        .EzfySlideshow-cell--{{ block.id}} .EzfySlideshow-button--2,
        .EzfySlideshow-cell--{{ block.id}} .EzfySlideshow-button--2:active,
        .EzfySlideshow-cell--{{ block.id}} .EzfySlideshow-button--2:hover,
        .EzfySlideshow-cell--{{ block.id}} .EzfySlideshow-button--2:focus{
          background-color: {{ button_background_color_2 }};
          color: {{ button_text_color_2 }};
          border-color: {{ button_border_color_2 }};
        }
        
        .EzfySlideshow-cell--{{ block.id}} .EzfySlideshow-title{
          color: {{ block.settings.title_color }} !important;
        }
        
                .EzfySlideshow-cell--{{ block.id}} .EzfySlideshow-subtitle{
          color: {{ block.settings.subtitle_color }} !important;
        }
        

        
      /* Customizations 
  	   ================================ */
      </style>
      
      
  </div>
  {% endfor %}


  <div class="EzfySlideshow-progress-container">
    <div class="EzfySlideshow-progress"></div>
  </div>
  
  </div>

 </div>
  <div class="EzfySlideshow-arrows">
    <button class="EzfySlideshow-prev flickity-button flickity-prev-next-button previous" type="button" aria-label="Previous"><svg class="flickity-button-icon" viewBox="0 0 100 100"><path d="M 10,50 L 60,100 L 70,90 L 30,50  L 70,10 L 60,0 Z" class="arrow"></path></svg></button>
  <button class="EzfySlideshow-next flickity-button flickity-prev-next-button next" type="button" aria-label="Next"><svg class="flickity-button-icon" viewBox="0 0 100 100"><path d="M 10,50 L 60,100 L 70,90 L 30,50  L 70,10 L 60,0 Z" class="arrow" transform="translate(100, 100) rotate(180) "></path></svg></button>
  </div>



{% comment %}Overall Styles{% endcomment %}

<style>
  
    .EzfySlideshow-container--loaded *{
  	opacity: 1;
  }
  
  .ezfy-slideshow-section{
  	position: relative;
  }
  
  

  
  .EzfySlideshow-container{
    max-width: 100%;
    width: 100%;
    position: relative;
    overflow: hidden;
  }
  

  
  .EzfySlideshow-container{
        position: relative;
        display: block;
  }
  
  
  @media (min-width: 750px){
        {% if natural_height %}
    [data-ezfy-slideshow-section-id="{{ section.id }}"] .EzfySlideshow-textbox-container--parent,
        [data-ezfy-slideshow-section-id="{{ section.id }}"].EzfySlideshow-container,
    [data-ezfy-slideshow-section-id="{{ section.id }}"] .EzfySlideshow-container,
    [data-ezfy-slideshow-section-id="{{ section.id }}"] .EzfySlideshow-parallax{
       height: 0 !important; 


    	padding-bottom: calc(100% / {{ natural_height_ratio }}); 

  }

    .EzfySlideshow-cell{
    	height: 100%;
    }
    
	[data-ezfy-slideshow-section-id="{{ section.id }}"] .EzfySlideshow-parallax{
             height: 0 !important; 
     	padding-bottom: calc(100% / {{ natural_height_ratio  }}); 
    }

    
    .EzfySlideshow-textbox-container--child {
    display: table;
    width: 100%;
    height: 100%;
    /* padding-bottom: calc(100% / 1.499267935578331); */
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
}
  
  
  {% else %}
  
  
      [data-ezfy-slideshow-section-id="{{ section.id }}"].EzfySlideshow-container,
  	  [data-ezfy-slideshow-section-id="{{ section.id }}"] .EzfySlideshow-cell,
    [data-ezfy-slideshow-section-id="{{ section.id }}"] .EzfySlideshow-image,
    [data-ezfy-slideshow-section-id="{{ section.id }}"] .EzfySlideshow-textbox-container--parent{

          height: {{ section_height }};
      max-height: {{ section_height }};


  
  }
   {% endif %}
  }
  
    
  @media (max-width: 749px){
  {% if natural_height_mobile %}
    
    	

    [data-ezfy-slideshow-section-id="{{ section.id }}"].EzfySlideshow-container,
    [data-ezfy-slideshow-section-id="{{ section.id }}"] .EzfySlideshow-container,
    [data-ezfy-slideshow-section-id="{{ section.id }}"] .EzfySlideshow-parallax{
       	height: 0 !important; 


   	padding-bottom: calc(100% / {{ natural_height_ratio_mobile  }}); 

  	}

    
    .EzfySlideshow-cell{
    	height: 100%;
    }
  
  
  
  {% else %}
    [data-ezfy-slideshow-section-id="{{ section.id }}"].EzfySlideshow-container,
  	[data-ezfy-slideshow-section-id="{{ section.id }}"] .EzfySlideshow-cell,
    [data-ezfy-slideshow-section-id="{{ section.id }}"] .EzfySlideshow-image,
    [data-ezfy-slideshow-section-id="{{ section.id }}"] .EzfySlideshow-textbox-container--parent{
        height: {{ section_height_mobile }};
      	max-height: {{ section_height_mobile }};
  	}

    {% endif %}
  }

  /* Pre-loader
  ============================= */
  
[data-ezfy-slideshow-section-id="{{ section.id }}"].EzfySlideshow-container--loading:before{
    left: 0;
    right: 0;
    content: '';
    top: 0;
    bottom: 0;
    background: {{ loader_background_color }};
    width: 100%;
    height: 100%;
    z-index: 1;
    position: absolute;
  }


  .EzfySlideshow-container--loading:after{
    opacity: 0;
    animation: preloading .5s ease .3s infinite;
    animation-delay: 1s;
    content: "";
    position: absolute;
    width: 100px;
    height: 3px;
    background: white;
    left: 50%;
    top: 50%;
    margin: -1px 0 0 -50px;
    z-index: 2;
  }
  


  .EzfySlideshow-container--loaded:after,
  .EzfySlideshow-container--loaded:before{
    opacity: 0 !important;
    transition: opacity .3s ease;
    
    pointer-events: none;
  }
 
  
  /* Cell
  ============================= */
  

  .EzfySlideshow-cell{
    width: 100%;
    position: relative;
    display: table;
  }
    
  .EzfySlideshow-cell:not(.is-selected) .lazyload{
  	display: none;
  }

  .flickity-viewport {
    overflow: hidden;
    position: relative;
/*     transition: height .6s; */
    height: 100%;
  }
  
  .EzfySlideshow-image-wrapper{
    position: relative;

    width: 100%;
    height: 100%;
}

  .EzfySlideshow-image{
    object-fit: cover;
    width: 100%;
    height: 100%;
    


  }

      {% if zoom_animation %}
  .EzfySlideshow-container--loaded .EzfySlideshow-cell:nth-child(1) img,
  .EzfySlideshow-container--loaded .EzfySlideshow-cell:nth-child(1) [class*='parallax']{
    animation: zoom-fade 2.3s cubic-bezier(.26,.54,.32,1) 0s forwards;
}

    {% endif %}
  
  @media only screen and (min-width: 750px){
    .EzfySlideshow-image--desktop,
    .EzfySlideshow-parallax--desktop{
    	display: block;
    }
    .EzfySlideshow-image--mobile,
    .EzfySlideshow-parallax--mobile{
    	display: none;
    }
  }
  
  @media only screen and (max-width: 749px){
    .EzfySlideshow-image--desktop,
    .EzfySlideshow-parallax--desktop{
    	display: none;
    }
    .EzfySlideshow-image--mobile,
    .EzfySlideshow-parallax--mobile{
    	display: block;
    }
  }

  .EzfySlideshow-image--dummy {
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    max-width: 100%;
    height: 100%;
    margin: 0 auto;
    
    fill: #999;
    background-color: #e1e1e1;
    width: 100%;

  }

  .EzfySlideshow ol li{
    list-style-type: none;
  }
  
  
  
  /* Parallax 
  ============================= */
  
  .EzfySlideshow-parallax{
    background-attachment: fixed;
  }
  
/*     [data-enable-parallax="true"] + div .EzfySlideshow-cell.is-selected .EzfySlideshow-parallax{
    background-attachment: fixed;
  }
   */
  
    .EzfySlideshow-parallax{

  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
    min-height: 100%;
  }
 
  
  .EzfySlideshow-parallax > img{
  	display: none;
  }
  

  
  /* Text
  ============================= */
  
  .EzfySlideshow-textbox-container--parent{
    
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
  
  }
  
   .EzfySlideshow-textbox-container--child{
    
   	display: table;
    width: 100%;
	height: 100%;
  
  }
    
  .EzfySlideshow-textbox{
/* 	position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%); */
    
    display: table-cell;
    vertical-align: middle;
    position: relative;
    padding: 15px 0;
    z-index: 4;
    max-width: 1500px;
    width: 100%;
    height: 100%;
    padding: 0 17px;
  }
  
  .EzfySlideshow-textbox *{
  text-transform: unset !important;
    letter-spacing: normal !important;
  }
  
  @media only screen and (min-width: 750px){
       .EzfySlideshow-textbox{
          padding: 0 40px;
    }
  }
  
  
  .EzfySlideshow-textbox *{
